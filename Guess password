a = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
b = "!@#$%^&*({})+-=<>?/\\\'\"._-~[]"
c = a + b
import random

e=True
while e==True:
    length = int(input("Enter length of your password: "))

    d = []
    for i in range(length):
        d.append(random.choice(c))
    # print(d)
    finalpass = "".join(d)
    print(finalpass)
    f = input("Do you want to continue?y/n ")
    if f.lower() == "n":
        break
    else:
        continue
